<% @studies.each do |study| %>
  <% study.outline[:subtitles].each do |subtitle, points| %>
    <ol>
    <li><h3><%= subtitle %></h3></li>
    <div class= "point">
    <ul>
      <% points.each do  |point| %>
        <li>
        <%= point %>
        </li>
       
      <% end %>
    </ul>
    </div>
    </ol>
  <% end %>
  
<% end %>

